<plugin>
    <name>apDeliveryCacheRedis</name>
    <displayName>AdserverPlugins.com Redis Delivery Cache</displayName>
    <creationDate>2015-02-23</creationDate>
    <author>AdserverPlugins.com</author>
    <authorEmail>info@adserverplugins.com</authorEmail>
    <authorUrl>http://www.adserverplugins.com/</authorUrl>
    <license>GPLv2</license>
    <description>Redis Delivery Cache Store Plugin</description>
    <version>1.3.2</version>
    <type>package</type>

    <install>

        <contents>
            <group name="apRedis">1</group>
        </contents>

        <register>
            <extension type='deliveryCacheStore' group='apRedis'>apRedis</extension>
        </register>

    </install>

</plugin>
